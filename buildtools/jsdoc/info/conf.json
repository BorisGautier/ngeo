{
    "opts": {
        "recurse": true,
        "template": "buildtools/jsdoc/info"
    },
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "includePattern": "\\.js$"
    },
    "plugins": [
        "buildtools/jsdoc/info/define-plugin",
        "buildtools/jsdoc/info/todo-plugin"
    ]
}
